    "ts-injecty": "0.0.22",
    "v-click-outside": "3.2.0",
    "vue": "2.7.16",
    "vue-demi": "0.14.10",
    "vue-svgicon": "3.3.2",
    "vue-template-compiler": "2.7.16",
    "vuedraggable": "2.24.3"
