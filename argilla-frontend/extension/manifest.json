{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Argilla io",
  "description": "Argilla io extension for development things",
  "author": "damian@argilla.io",
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Argilla io",
    "default_popup": "popup.html"
  }
}
