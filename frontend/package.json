{
  "name": "rubrix",
  "version": "0.1.2",
  "private": true,
  "eslintIgnore": [
    "node_modules/**/*",
    ".nuxt/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && cypress install --force && npm run test:e2e:run",
    "generate-icons": "vsvg -s ./static/icons -t ./assets/icons",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore  .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .eslintignore  .",
    "precommit": "yarn lintfix",
    "e2e": "cypress open --browser chrome",
    "e2e:silent": "cypress run"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1613647907.37b1156",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vuex-orm/core": "^0.36.3",
    "@vuex-orm/plugin-axios": "^0.9.3",
    "core-js": "^3.6.5",
    "moment-timezone": "^0.5.33",
    "node-sass": "^5.0.0",
    "nuxt": "^2.14.6",
    "sass-loader": "^10.1.0",
    "v-click-outside": "^3.1.2",
    "vue-moment": "^4.1.0",
    "vue-svgicon": "^3.2.9",
    "vue-vega": "^1.0.0-alpha.13"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^6.2.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.4.1"
  }
}
