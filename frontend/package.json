{
  "name": "rubrix",
  "version": "0.8.0",
  "private": true,
  "eslintIgnore": [
    "node_modules/**/*",
    ".nuxt/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate && cypress install --force && npm run test:e2e:run",
    "generate-icons": "vsvg -s ./static/icons -t ./assets/icons --tpl ./assets/icon-template.js.tmp",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore  .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .eslintignore  .",
    "precommit": "yarn lintfix",
    "e2e": "cypress open --browser chrome",
    "e2e:silent": "cypress run",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1613647907.37b1156",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vuex-orm/core": "^0.36.3",
    "@vuex-orm/plugin-axios": "^0.9.3",
    "core-js": "^3.6.5",
    "moment-timezone": "^0.5.33",
    "node-sass": "^5.0.0",
    "nuxt": "^2.14.6",
    "nuxt-highlightjs": "^1.0.1",
    "sass-loader": "^10.1.0",
    "stringz": "^2.1.0",
    "v-click-outside": "^3.1.2",
    "vue-moment": "^4.1.0",
    "vue-svgicon": "^3.2.9",
    "vue-vega": "^1.0.0-alpha.13",
    "vue-virtual-scroller": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@nuxt/test-utils": "^0.2.2",
    "@vue/test-utils": "^1.2.2",
    "axios-mock-adapter": "^1.19.0",
    "babel-core": "7.0.0-bridge.0",
    "cypress": "^6.2.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.5",
    "jest-serializer-vue": "^2.0.2",
    "prettier": "^2.2.1",
    "vue-jest": "^3.0.7"
  }
